<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.chenyunlong.qing.mapper.BilibiliAnimeScoreMapper">

    <resultMap id="BaseResultMap" type="cn.chenyunlong.qing.model.entities.bilibili.BilibiliAnimeScoreEntity">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="animeId" column="anime_id" jdbcType="BIGINT"/>
        <result property="score" column="score" jdbcType="DOUBLE"/>
        <result property="recordTime" column="record_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,anime_id,score,
        record_time
    </sql>
</mapper>
