<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.chenyunlong.zhangli.mapper.SignMapper">
    <insert id="add" parameterType="cn.chenyunlong.zhangli.model.entities.Sign">
        INSERT INTO `zhangli`.`t_sign_record` (`user_id`, `date_month`, `mask`, `continue_sign_month`)
        VALUES (#{userId},
                #{dateMonth}, #{mask}, #{continueSignMonth})
    </insert>
</mapper>
